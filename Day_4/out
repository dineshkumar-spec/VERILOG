#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5aff53345490 .scope module, "while_count_ones" "while_count_ones" 2 1;
 .timescale 0 0;
v0x5aff533457f0_0 .var/i "count", 31 0;
v0x5aff53390ec0_0 .var "data", 7 0;
v0x5aff53390fa0_0 .var/i "i", 31 0;
S_0x5aff53345640 .scope module, "while_loop_example" "while_loop_example" 3 1;
 .timescale 0 0;
v0x5aff53391060_0 .var "data", 7 0;
v0x5aff53391160_0 .var/i "shifts", 31 0;
    .scope S_0x5aff53345490;
T_0 ;
    %pushi/vec4 173, 0, 8;
    %store/vec4 v0x5aff53390ec0_0, 0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aff533457f0_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aff53390fa0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5aff53390fa0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5aff53390ec0_0;
    %load/vec4 v0x5aff53390fa0_0;
    %part/s 1;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x5aff533457f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5aff533457f0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x5aff53390fa0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5aff53390fa0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 17 "$display", "Data = %b, Number of 1s = %0d", v0x5aff53390ec0_0, v0x5aff533457f0_0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5aff53345640;
T_1 ;
    %pushi/vec4 176, 0, 8;
    %store/vec4 v0x5aff53391060_0, 0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5aff53391160_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x5aff53391060_0;
    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz T_1.1, 4;
    %load/vec4 v0x5aff53391060_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftr 4;
    %store/vec4 v0x5aff53391060_0, 0, 8;
    %load/vec4 v0x5aff53391160_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5aff53391160_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 3 12 "$display", "Shifted until zero in %0d steps", v0x5aff53391160_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
