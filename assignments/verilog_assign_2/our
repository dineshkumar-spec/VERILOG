#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5dd85d966590 .scope module, "time_var" "time_var" 2 5;
 .timescale 0 0;
v0x5dd85d966720_0 .var "t", 63 0;
    .scope S_0x5dd85d966590;
T_0 ;
    %vpi_func 2 8 "$time" 64 {0 0 0};
    %store/vec4 v0x5dd85d966720_0, 0, 64;
    %vpi_call 2 9 "$display", "Time = %0t", $time, v0x5dd85d966720_0 {0 0 0};
    %delay 5, 0;
    %vpi_func 2 11 "$time" 64 {0 0 0};
    %store/vec4 v0x5dd85d966720_0, 0, 64;
    %vpi_call 2 12 "$display", "Time = %0t", $time, v0x5dd85d966720_0 {0 0 0};
    %delay 10, 0;
    %vpi_func 2 14 "$time" 64 {0 0 0};
    %store/vec4 v0x5dd85d966720_0, 0, 64;
    %vpi_call 2 15 "$display", "Time = %0t", $time, v0x5dd85d966720_0 {0 0 0};
    %delay 20, 0;
    %vpi_func 2 17 "$time" 64 {0 0 0};
    %store/vec4 v0x5dd85d966720_0, 0, 64;
    %vpi_call 2 18 "$display", "Time = %0t", $time, v0x5dd85d966720_0 {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "time_var.v";
