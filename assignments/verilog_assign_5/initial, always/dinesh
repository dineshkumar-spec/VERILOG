    %store/vec4 v0x6177812f9af0_0, 0, 4;
    %vpi_call 2 7 "$display", "time=%0t|a=%0h|b=%0h|c=%0h", $time, v0x6177812af6a0_0, v0x6177812f9a10_0, v0x6177812f9af0_0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x6177812af510;
T_1 ;
    %delay 10, 0;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x6177812af6a0_0, 0, 4;
    %delay 15, 0;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x6177812f9a10_0, 0, 4;
    %end;
    .thread T_1;
    .scope S_0x6177812af510;
T_2 ;
    %delay 20, 0;
    %pushi/vec4 9, 0, 4;
    %store/vec4 v0x6177812f9af0_0, 0, 4;
    %end;
    .thread T_2;
    .scope S_0x6177812af510;
T_3 ;
    %vpi_call 2 19 "$monitor", "time=%0t|a=%0h|b=%0h|c=%0h", $time, v0x6177812af6a0_0, v0x6177812f9a10_0, v0x6177812f9af0_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x6177812af510;
T_4 ;
    %delay 60, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_4;
