#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5f26110b5570 .scope module, "two_always" "two_always" 2 1;
 .timescale 0 0;
v0x5f26110b5700_0 .var "a", 0 0;
v0x5f26110fd5a0_0 .var "b", 0 0;
    .scope S_0x5f26110b5570;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f26110b5700_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x5f26110b5570;
T_1 ;
    %delay 8, 0;
    %load/vec4 v0x5f26110b5700_0;
    %inv;
    %store/vec4 v0x5f26110b5700_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5f26110b5570;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f26110fd5a0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5f26110b5570;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x5f26110fd5a0_0;
    %inv;
    %store/vec4 v0x5f26110fd5a0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5f26110b5570;
T_4 ;
    %vpi_call 2 10 "$monitor", "time=%0t|a=%b|b=%b", $time, v0x5f26110b5700_0, v0x5f26110fd5a0_0 {0 0 0};
    %delay 40, 0;
    %vpi_call 2 11 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "two_always.v";
