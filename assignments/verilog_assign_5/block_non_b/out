#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x621d1ef61570 .scope module, "mix_example" "mix_example" 2 1;
 .timescale 0 0;
v0x621d1ef61720_0 .var "a", 0 0;
v0x621d1efa8b60_0 .var "b", 0 0;
v0x621d1efa8c20_0 .var "clk", 0 0;
E_0x621d1ef62650 .event posedge, v0x621d1efa8c20_0;
    .scope S_0x621d1ef61570;
T_0 ;
    %wait E_0x621d1ef62650;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x621d1ef61720_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x621d1efa8b60_0, 0, 1;
    %load/vec4 v0x621d1ef61720_0;
    %store/vec4 v0x621d1efa8b60_0, 0, 1;
    %load/vec4 v0x621d1efa8b60_0;
    %assign/vec4 v0x621d1ef61720_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x621d1ef61570;
T_1 ;
    %vpi_call 2 10 "$monitor", "a=%b | b=%b", v0x621d1ef61720_0, v0x621d1efa8b60_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x621d1efa8c20_0, 0, 1;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
