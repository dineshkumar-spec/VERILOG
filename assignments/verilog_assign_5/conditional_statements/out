S_0x61347ad9e080 .scope module, "tb_siso" "tb_siso" 2 1;
 .timescale 0 0;
v0x61347adb27f0_0 .var "clk", 0 0;
v0x61347adb28b0_0 .var "rst", 0 0;
v0x61347adb2980_0 .var "si", 0 0;
v0x61347adb2a80_0 .net "so", 0 0, v0x61347adb26b0_0;  1 drivers
S_0x61347ad9e210 .scope module, "dut" "siso_shift_reg" 2 6, 3 1 0, S_0x61347ad9e080;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "si";
    .port_info 3 /OUTPUT 1 "so";
v0x61347ad63490_0 .net "clk", 0 0, v0x61347adb27f0_0;  1 drivers
v0x61347adb2290_0 .var "q0", 0 0;
v0x61347adb2350_0 .var "q1", 0 0;
v0x61347adb2420_0 .var "q2", 0 0;
v0x61347adb24e0_0 .net "rst", 0 0, v0x61347adb28b0_0;  1 drivers
v0x61347adb25f0_0 .net "si", 0 0, v0x61347adb2980_0;  1 drivers
v0x61347adb26b0_0 .var "so", 0 0;
E_0x61347ad9c730 .event posedge, v0x61347adb24e0_0, v0x61347ad63490_0;
    .scope S_0x61347ad9e210;
T_0 ;
    %wait E_0x61347ad9c730;
    %load/vec4 v0x61347adb24e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x61347adb2290_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x61347adb2350_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x61347adb2420_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x61347adb26b0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x61347adb2350_0;
    %assign/vec4 v0x61347adb2420_0, 0;
    %load/vec4 v0x61347adb2290_0;
    %assign/vec4 v0x61347adb2350_0, 0;
    %load/vec4 v0x61347adb25f0_0;
    %assign/vec4 v0x61347adb2290_0, 0;
    %load/vec4 v0x61347adb2420_0;
    %assign/vec4 v0x61347adb26b0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x61347ad9e080;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61347adb27f0_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x61347adb27f0_0;
    %inv;
    %store/vec4 v0x61347adb27f0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x61347ad9e080;
T_2 ;
    %vpi_call 2 15 "$monitor", "Time=%0t | si=%b | so=%b", $time, v0x61347adb2980_0, v0x61347adb2a80_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61347adb28b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61347adb2980_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61347adb28b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61347adb2980_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61347adb2980_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61347adb2980_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x61347adb2980_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x61347adb2980_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
