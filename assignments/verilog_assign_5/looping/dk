#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x59c19d9c15a0 .scope module, "forever_loop" "forever_loop" 2 1;
 .timescale 0 0;
v0x59c19d9e7720_0 .var "clk", 0 0;
S_0x59c19d9c1730 .scope begin, "clk_gen" "clk_gen" 2 5, 2 5 0, S_0x59c19d9c15a0;
 .timescale 0 0;
    .scope S_0x59c19d9c15a0;
T_0 ;
    %fork t_1, S_0x59c19d9c15a0;
    %fork t_2, S_0x59c19d9c15a0;
    %join;
    %join;
    %jmp t_0;
t_1 ;
    %fork t_4, S_0x59c19d9c1730;
    %jmp t_3;
    .scope S_0x59c19d9c1730;
t_4 ;
T_0.0 ;
    %delay 5, 0;
    %load/vec4 v0x59c19d9e7720_0;
    %inv;
    %store/vec4 v0x59c19d9e7720_0, 0, 1;
    %jmp T_0.0;
    %end;
    .scope S_0x59c19d9c15a0;
t_3 %join;
    %end;
t_2 ;
    %delay 100, 0;
    %disable S_0x59c19d9c1730;
    %end;
    .scope S_0x59c19d9c15a0;
t_0 ;
    %end;
    .thread T_0;
    .scope S_0x59c19d9c15a0;
T_1 ;
    %vpi_call 2 14 "$monitor", "time=%0t|clk=%b", $time, v0x59c19d9e7720_0 {0 0 0};
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "forever_loop.v";
