#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x56185e16e590 .scope module, "blocking" "blocking" 2 1;
 .timescale 0 0;
v0x56185e16e720_0 .var "a", 0 0;
v0x56185e1b5d20_0 .var "b", 0 0;
    .scope S_0x56185e16e590;
T_0 ;
    %vpi_call 2 4 "$monitor", "time=%0t | a=%b | b=%b", $time, v0x56185e16e720_0, v0x56185e1b5d20_0 {0 0 0};
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56185e16e720_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56185e1b5d20_0, 0, 1;
    %delay 20, 0;
    %vpi_call 2 7 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "blocking.v";
