#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x58abf8c6d7d0 .scope module, "tb_half_adder" "tb_half_adder" 2 1;
 .timescale 0 0;
v0x58abf8c80d20_0 .var "a", 0 0;
v0x58abf8c80de0_0 .var "b", 0 0;
v0x58abf8c80eb0_0 .net "carry", 0 0, L_0x58abf8c811e0;  1 drivers
v0x58abf8c80fb0_0 .net "sum", 0 0, L_0x58abf8c81080;  1 drivers
S_0x58abf8c6d960 .scope module, "dut" "half_adder" 2 4, 3 1 0, S_0x58abf8c6d7d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "sum";
    .port_info 3 /OUTPUT 1 "carry";
L_0x58abf8c81080 .functor XOR 1, v0x58abf8c80d20_0, v0x58abf8c80de0_0, C4<0>, C4<0>;
L_0x58abf8c811e0 .functor AND 1, v0x58abf8c80d20_0, v0x58abf8c80de0_0, C4<1>, C4<1>;
v0x58abf8c6dbb0_0 .net "a", 0 0, v0x58abf8c80d20_0;  1 drivers
v0x58abf8c80a50_0 .net "b", 0 0, v0x58abf8c80de0_0;  1 drivers
v0x58abf8c80b10_0 .net "carry", 0 0, L_0x58abf8c811e0;  alias, 1 drivers
v0x58abf8c80bb0_0 .net "sum", 0 0, L_0x58abf8c81080;  alias, 1 drivers
    .scope S_0x58abf8c6d7d0;
T_0 ;
    %vpi_call 2 6 "$dumpfile", "half_adder.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x58abf8c6d7d0 {0 0 0};
    %vpi_call 2 8 "$monitor", "Time=%0t | a=%b | b=%b | sum=%b | carry=%b", $time, v0x58abf8c80d20_0, v0x58abf8c80de0_0, v0x58abf8c80fb0_0, v0x58abf8c80eb0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58abf8c80d20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58abf8c80de0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58abf8c80d20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58abf8c80de0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58abf8c80d20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58abf8c80de0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58abf8c80d20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58abf8c80de0_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_half_adder.v";
