    %store/vec4 v0x62453b5a0040_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62453b5a0100_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62453b5a0040_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62453b5a0100_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62453b5a0040_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62453b5a0100_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62453b5a0040_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62453b5a0100_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
