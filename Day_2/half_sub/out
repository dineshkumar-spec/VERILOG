S_0x62453b58c810 .scope module, "dut" "half_sub" 2 4, 3 1 0, S_0x62453b579740;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "diff";
    .port_info 3 /OUTPUT 1 "bor";
L_0x62453b5a03a0 .functor XOR 1, v0x62453b5a0040_0, v0x62453b5a0100_0, C4<0>, C4<0>;
L_0x62453b5a0500 .functor NOT 1, v0x62453b5a0040_0, C4<0>, C4<0>, C4<0>;
