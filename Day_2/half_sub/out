v0x62453b59fc90_0 .net "a", 0 0, v0x62453b5a0040_0;  1 drivers
v0x62453b59fd50_0 .net "b", 0 0, v0x62453b5a0100_0;  1 drivers
v0x62453b59fdf0_0 .net "bor", 0 0, L_0x62453b5a0570;  alias, 1 drivers
v0x62453b59feb0_0 .net "diff", 0 0, L_0x62453b5a03a0;  alias, 1 drivers
    .scope S_0x62453b579740;
T_0 ;
    %vpi_call 2 6 "$dumpfile", "half_sub.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x62453b579740 {0 0 0};
    %vpi_call 2 8 "$monitor", "Time=%0t | a=%b | b=%b | diff=%b | bor=%b", $time, v0x62453b5a0040_0, v0x62453b5a0100_0, v0x62453b5a02d0_0, v0x62453b5a01d0_0 {0 0 0};
